{
    "name": "spur1-media/spur1-feature-bundle",
    "description": "Symfony Bundle | Provides feature flag functionality for SPUR1 applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Spur1\\FeatureBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "wolfkidsounds",
            "email": "eric@pulsationaudio.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "symfony/framework-bundle": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/dotenv": "^7.2",
        "symfony/expression-language": "^7.2"
    },
    "require-dev": {
        "symfony/runtime": "^7.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
